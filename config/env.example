# Development Environment Example
# Copy this file to .env.dev and fill in your settings
# This example file CAN be committed to Git, actual .env.dev should NOT

# Google Apps Script Web App URL
GAS_BASE_URL=YOUR_GAS_URL_HERE

# Environment mode
ENVIRONMENT=dev

# Enable debug logging
DEBUG_LOG=true

# Central Weather Administration API Key
CWA_API_KEY=YOUR_CWA_API_KEY_HERE

# CWA API Host (Default: https://opendata.cwa.gov.tw, Web builds use /cwa-proxy)
CWA_API_HOST=https://opendata.cwa.gov.tw
